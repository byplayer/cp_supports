{
  "name": "cp-supports",
  "displayName": "CP Supports",
  "description": "Competitive Programming support tools for VSCode",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yukio-goto/cp_supports"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cp-supports.openCaseFirst",
        "title": "Open First Test Case"
      },
      {
        "command": "cp-supports.openCase",
        "title": "Open Test Case"
      },
      {
        "command": "cp-supports.openCPFiles",
        "title": "Open CP Files"
      },
      {
        "command": "cp-supports.createTestCases",
        "title": "Create Test Cases"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "cp-supports.openCPFiles",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "cp-supports.createTestCases",
          "when": "explorerResourceIsFolder && resourceFilename == test",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "package": "vsce package"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^9.15.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.15.0"
  }
}
